  <meta property="og:image" content="https://the-noah.github.io/sectors-edge-tournament/social.png">
  <title>Sector's Edge Guide</title>
  <style><%- css.style -%></style>
</head>
<body>
  <div class="section">
    <h1>Sector's Edge Guide</h1>
    <p>Written guide to Sector's Edge, a brand new free-to-play first person shooter with a completely destructible environment.</p>
  </div>
  <% categories.forEach((category) => { %>
    <div class="section category">
      <h2><%= category %></h2><br>
      <ul>
        <% guides.filter((guide) => guide.category === category).forEach((guide) => { %>
          <li><a href="#<%= guide.name.toLowerCase().replace(/ /g, "-") %>"><%= guide.name %></a></li>
        <% }) %>
      </ul>
    <% guides.filter((guide) => guide.category === category).forEach((guide) => { %>
      <div id="<%= guide.name.toLowerCase().replace(/ /g, "-") %>">
        <h1><%= guide.name %></h1>
        <p><%= guide.description %></p>
        <h2>Win</h2>
        <p><%= guide.win %></p>
      </div>
    <% }) %>
    </div>
  <% }) %>
  <span style="flex:1"></span>
  <footer>
    <div>
      <div>
        <span>Navigation</span>
        <a class="animated" href="#">Home</a>
